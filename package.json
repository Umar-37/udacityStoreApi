{
  "name": "db",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "nodemon": "nodemon",
    "test": "npm run upTest && npm run build && NODE_ENV=test ENV=test jasmine && npm run downTest || npm run downTest ",
    "create-test-db": "db-migrate --config database.json db:create test",
    "prebuild": "rm -rf build",
    "build": "tsc -p ./tsconfig.json",
    "prestart": "npm run build",
    "start": "node ./build/index",
    "upDev": "db-migrate up -e dev",
    "upTest": "npm run create-test-db && db-migrate up -e test",
    "downDev": "db-migrate down -e dev",
    "downTest": "db-migrate down -e test && db-migrate db:drop test ",
    "jasmine": "jasmine -ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.14",
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "node-ts": "^5.1.2",
    "pg": "^8.8.0",
    "supertest": "^6.3.1",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jasmine": "^4.3.0",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "bcrypt": "^5.1.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3"
  }
}
